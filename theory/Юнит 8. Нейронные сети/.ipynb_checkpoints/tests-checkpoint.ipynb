    "import tensorflow as tf\n",
    "\n",
    "def scheduler(epoch, lr):\n",
    "    if epoch < 4:\n",
    "        return lr\n",
    "    else:\n",
    "        return lr * tf.math.exp(-0.1)\n",
    "\n",
    "lr = 0.01\n",
    "for i in range(13):\n",
    "    lr = scheduler(i+1, lr)\n",
    "    print(lr)"
   ]
